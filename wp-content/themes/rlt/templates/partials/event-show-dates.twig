

    {% if post.end_date != post.start_date %}
                            
        {% if post.end_date | date("Y") != post.start_date | date("Y") %}
            {{ post.get_field('start_date') }}
        {% else %}
            {{ post.get_field('start_date') | date("F j") }}
        {% endif %}
        
        {% if post.end_date | date("m") != post.start_date | date("m") %}
            - {{ post.get_field('end_date') }}
        {% else %}
            - {{ post.get_field('end_date') | date("j, Y") }}
        {% endif %}
         
    {% else %}
        {{ post.get_field('start_date') }}
    {% endif %}